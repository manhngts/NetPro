  "ConfigPath": "config.json", //json配置文件的统一存放路径，建议一个模块，或者一个组件对应一个json文件
  "Overridable": true, //是否允许二次加载appsetting.环境.json ,默认true
  "NetProOption": {
    "UseResponseCompression": true, //是否启用响应压缩
    "RequestWarningThreshold": 5, //请求时长的警告临界值
    "RoutePrefix": "api", //全局路由前缀
    "IsDebug": true
  },
  "TypeFinderOption": {
    "CustomDllPattern": "^XXX.*",
    "MountePath": "C:/data/netpro/"
  }, 
  "AllowedHosts": "*" //主机筛选默认禁用
}